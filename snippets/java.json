{
  "niagara_type": {
    "prefix": "@type",
    "body":[ "@NiagaraType" ]
  },
  "niagara singelton": {
    "prefix": "@singelton",
    "body": [ "@NiagaraSingelton" ]
  },
  "new property": {
    "prefix": "@property",
    "body": [
      "@NiagaraProperty(",
      "\tname=\"${1:name}\",",
      "\ttype=\"${2:module}:${3:type}\",",
      "\tdefaultValue=\"${4:defaultValue}\",",
      "\tflags=\"${5:flags}\"",
      ")"
    ],
    "description" : "inserts a empty Niagara Property"
  },
  "out property": {
    "prefix": "@property",
    "body": [
      "@NiagaraProperty(",
      "\tname=\"out\",",
      "\ttype=\"${1:module}:${2:type}\",",
      "\tdefaultValue=\"${3:defaultValue}\",",
      "\tflags=\"${4:flags}\"",
      ")"
    ],
    "description": "inserts a out Niagara Property"
  },
  "in property": {
    "prefix": "@property",
    "body": [
      "@NiagaraProperty(",
      "\tname=\"in\",",
      "\ttype=\"${1:module}:${2:type}\",",
      "\tdefaultValue=\"${3:defaultValue}\",",
      "\tflags=\"${4:flags}\"",
      ")"
    ],
    "description": "inserts a in Niagara Property"
  },
  "new action": {
    "prefix": "@action",
    "body": [
      "@NiagaraAction(",
      "\tname=\"${1:Name}\"",
      ")"
    ],
    "description": "insert a simple Niagara Action"
  },
  "new topic": {
    "prefix": "@topic",
    "body": [
      "@NiagaraTopic(",
      "\tname=\"${1:Name}\",",
      "\teventType=\"${2:eventType}\"",
      ")"
    ]
  },
  "niagara fozen enum": {
    "prefix": "@enum",
    "description": "Class with Niagara Type",
    "body": [
      "@NiagaraEnum(",
      "\trange = {",
      "\t\t@Range(value=$3)",
      "",
      "\t}",
      ")"
    ]
  }
}
