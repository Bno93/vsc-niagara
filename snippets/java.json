{
  "new property": {
    "prefix": "@property",
    "body": "@NiagaraProperty(name=\"${1:name}\", type=\"${2:module}:${3:type}\", defaultValue=\"${4:defaultValue}\", flags=\"${5:flags}\")",
    "description" : "inserts a empty Niagara Property"
  },
  "out property": {
    "prefix": "@property",
    "body": "@NiagaraProperty(name=\"out\", type=\"${1:module}:${2:type}\", defaultValue=\"${3:defaultValue}\", flags=\"${4:flags}\")",
    "description": "inserts a out Niagara Property"
  },
  "in property": {
    "prefix": "@property",
    "body": "@NiagaraProperty(name=\"in\", type=\"${1:module}:${2:type}\", defaultValue=\"${3:defaultValue}\", flags=\"${4:flags}\")",
    "description": "inserts a in Niagara Property"
  },
  "new action": {
    "prefix": "@action",
    "body": "@NiagaraAction(name=\"${1:Name}\")",
    "description": "insert a simple Niagara Action"
  },
  
  "niagara class": {
    "prefix": "class",
    "description": "Class with Niagara Type",
    "body": [
      "@NiagaraType",
      "public class $TM_FILENAME_BASE extends ${2:BComponent} {",
      "\t$0",
      "}"
    ]
  }

}